{
  "name": "stellar-turret-sdk",
  "description": "An SDK to interact with stellar-turrets",
  "version": "0.0.0",
  "author": "Julian <julian@hacksur.com> (http://hacksur.com)",
  "bugs": {
    "url": "https://github.com/hacksur/stellar-turret-sdk/issues",
    "email": "julian@hacksur.com"
  },
  "contributors": [
    "Julian <julian@hacksur.com> (http://hacksur.com)"
  ],
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@types/node": "^16.11.9",
    "ava": "4.0.0-rc.1",
    "bignumber.js": "^4.1.0",
    "codecov": "latest",
    "concurrently": "^6.4.0",
    "cross-env": "latest",
    "dotenv": "^10.0.0",
    "eslint": "latest",
    "eslint-config-xo-lass": "latest",
    "fetch-blob": "^3.1.3",
    "fixpack": "latest",
    "form-data": "^4.0.0",
    "formdata-node": "^4.3.1",
    "formdata-polyfill": "^4.0.10",
    "husky": "latest",
    "lint-staged": "latest",
    "node-fetch": "^2.6.6",
    "nyc": "latest",
    "remark-cli": "latest",
    "remark-preset-github": "latest",
    "stellar-sdk": "^9.1.0",
    "toml": "^2.3.6",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2",
    "xo": "latest"
  },
  "engines": {
    "node": ">= 10"
  },
  "homepage": "https://github.com/hacksur/stellar-turret-sdk",
  "keywords": [
    "lass",
    "stellar-turret-sdk"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hacksur/stellar-turret-sdk"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "watch-build": "tsc -w",
    "watch-test": "ava --watch",
    "dev": "concurrently 'npm:watch-*'",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "xo",
    "lint:md": "remark . -qfo",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test ava",
    "test-coverage": "cross-env NODE_ENV=test nyc npm run test"
  },
  "ava": {
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/"
      },
      "compile": false
    },
    "files": [
      "src/tests/**/*"
    ]
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ]
  },
  "dependencies": {
    "axios": "^0.21.4"
  }
}
